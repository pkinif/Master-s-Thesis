@article{Bromanframeworkstrategicsustainable2017,
  title = {A Framework for Strategic Sustainable Development},
  volume = {140},
  issn = {0959-6526},
  url = {http://www.sciencedirect.com/science/article/pii/S0959652615015930},
  doi = {10.1016/j.jclepro.2015.10.121},
  abstract = {The purpose of this paper is to give a comprehensive and cohesive description of the most recent version of the Framework for Strategic Sustainable Development (FSSD), and also to describe and discuss the overall method for developing the FSSD, elaborate on the general rational for and general benefits of a framework of this type, and validate benefits of the FSSD through examples of its application. The purpose is also to point to pertinent future work. In preparation of this paper, we have reviewed previous publications and other documents related to the FSSD and reflected on the 25-year learning process between scientists and practitioners. We conclude that the FSSD has proven to aid organizations in thoroughly understanding and putting themselves in context of the global sustainability challenge, and to move themselves strategically towards sustainability, i.e., to stepwise reduce their negative impacts on ecological and social systems at large while strengthening the own organization through capturing of innovation opportunities, including new business models, exploration of new markets and winning of new market shares, and through reduced risks and operation costs. Specifically, we conclude that the FSSD aids more effective management of system boundaries and trade-offs, makes it possible to model and assess sustainable potentials for various materials and practices before investments are made, and offers the possibility for more effective collaboration across disciplines and sectors, regions, value-chains and stakeholder groups. We also conclude that the FSSD makes it possible to prevent damages, even from yet unknown problems, and not the least, to guide selection, development and combination of supplementary methods, tools, and other forms of support, which makes it possible to increase their utility for strategic sustainable development. Finally, we have shown that the FSSD is useful for structuring transdisciplinary academic education and research. Several examples of ongoing FSSD related research, as well as ideas for future work, are given.},
  journaltitle = {Journal of Cleaner Production},
  shortjournal = {Journal of Cleaner Production},
  series = {Systematic Leadership towards Sustainability},
  date = {2017-01-01},
  pages = {17-31},
  keywords = {Backcasting,FSSD,Strategic sustainable development,Sustainability principles,Sustainability science},
  author = {Broman, G{\"o}ran Ingvar and Rob{\`e}rt, Karl-Henrik},
  file = {C:\\Users\\Pierrik\\Zotero\\storage\\I4TSPP8T\\Broman et Rob√®rt - 2017 - A framework for strategic sustainable development.pdf;C:\\Users\\Pierrik\\Zotero\\storage\\UFDPD26B\\S0959652615015930.html}
