@article{Cavaco2014,
  title = {{{CSR}} and Financial Performance: Complementarity between Environmental, Social and Business Behaviours},
  volume = {46},
  issn = {0003-6846},
  url = {https://doi.org/10.1080/00036846.2014.927572},
  doi = {10.1080/00036846.2014.927572},
  shorttitle = {{{CSR}} and Financial Performance},
  abstract = {This article analyses the interactions between various dimensions of corporate social responsibility (CSR) that mediate the relationship between CSR and financial performance. We hypothesize that the absence of consensus in the empirical literature on the CSR\textendash{}financial performance relationship may be explained by the existence of synergies (complementarity) and trade-offs (substitutability) between the different CSR components. We investigate such relationship using a final unbalanced panel sample of 1094 observations (around 300 firms per year) from 15 countries over the 2002\textendash{}2007 period. Our results show that responsible behaviours towards employees (human resources dimension) and towards customers and suppliers (business behaviour dimension) appear as complementary inputs of financial performance, indicating mutual benefits and less conflict between those stakeholders. Conversely, responsible behaviours towards customers and suppliers and towards the environment appear as substitutable inputs of financial performance, suggesting more conflict between or over-investment towards those stakeholders.},
  number = {27},
  journaltitle = {Applied Economics},
