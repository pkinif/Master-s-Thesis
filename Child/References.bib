  file = {C:\\Users\\Pierrik\\Zotero\\storage\\Z9BTKDVG\\Horbach et al. - 2012 - Determinants of eco-innovations by type of environ.pdf;C:\\Users\\Pierrik\\Zotero\\storage\\GJUE9ASN\\S0921800912001358.html}
