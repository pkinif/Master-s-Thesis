  file = {C:\\Users\\Pierrik\\Zotero\\storage\\Q634J798\\Vringer et al. - 2017 - Sustainable Consumption Dilemmas.pdf;C:\\Users\\Pierrik\\Zotero\\storage\\SHTNUDPK\\942.html}
